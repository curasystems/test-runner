    "diff": "^1.0.8",
    "glob": "^3.2.9",
    "growl": "~1.7.0",
    "lodash.defaults": "^2.4.1",
    "mocha": "^1.18.2",
    "optimist": "^0.6.1"
  },
  "scripts": {
    "test": "mocha"
  },
  "repository": {
