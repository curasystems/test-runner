    "diff": "^2.2.1",
    "glob": "^6.0.4",
    "growl": "^1.8.1",
    "lodash.defaults": "^4.0.0",
    "mocha": "^2.3.4",
    "optimist": "^0.6.1"
  },
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/curasystems/test-runner.git"
  },
  "keywords": [
